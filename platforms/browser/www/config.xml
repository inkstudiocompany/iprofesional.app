<?xml version='1.0' encoding='utf-8'?>
<widget id="com.iprofesional.app" version="1.0.7" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>iProfesional</name>
    <description />
    <author email="inkstudiocompany@gmail.com" href="http://www.inkstudio.com.co">Ink Studio</author>
    <content src="index.html" />
    <preference name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <preference name="android.permission.ACCESS_NETWORK_STATE" />
