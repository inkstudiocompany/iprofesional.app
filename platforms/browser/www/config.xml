    <gap:splash gap:platform="winphone" src="www/res/screen/windows-phone/screen-portrait.jpg" />
    <access origin="*" subdomains="true" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="unsafe:whatsapp://*" />
    <allow-intent href="http://ads.e-planning.net/*" />
    <allow-intent href="http://us.img.e-planning.net/*" />
    <allow-navigation href="http://www.iprofesional.com/*" />
    <allow-navigation href="http://iprofesional.com/*" />
    <allow-navigation href="http://ads.e-planning.net/*" />
    <allow-navigation href="http://us.img.e-planning.net/*" />
